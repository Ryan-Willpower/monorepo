{
  "version": 2,
  "projects": {
    "frontend": {
      "root": "frontend",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "options": {
            "outputPath": "frontend/.next",
            "root": "frontend"
          }
        },
        "dev": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "frontend:build",
            "port": 3000
          }
        }
      },
      "implicitDependencies": []
    },
    "shared": {
      "root": "shared",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-script",
          "options": {
            "script": "build"
          }
        }
      },
      "implicitDependencies": []
    }
  }
}
